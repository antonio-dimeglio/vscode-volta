{
  "name": "vscode-volta",
  "displayName": "vscode-volta",
  "description": "A simple extension for the volta programming language.",
  "version": "0.0.1",
  "icon": "images/volta-icon.svg",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "volta",
      "aliases": ["Volta", "volta"],
      "extensions": [".vlt"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./images/volta-icon.svg",
        "dark": "./images/volta-icon.svg"
      }
    }],
    "grammars": [{
      "language": "volta",
      "scopeName": "source.volta",
      "path": "./syntaxes/volta.tmLanguage.json"
    }],
    "snippets": [{
      "language": "volta",
      "path": "./snippets/volta.json"
    }]
  }
}
